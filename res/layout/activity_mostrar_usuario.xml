<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MostrarUsuario">

    <TextView
        android:id="@+id/lblTitulo2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:text="Usuario"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/lblNombre"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="46dp"
        android:text="Nombre:"
        app:layout_constraintBottom_toBottomOf="@+id/lblApellido"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/lblApellido" />

    <TextView
        android:id="@+id/lblMostrarNombre"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="19dp"
        android:text="----"
        app:layout_constraintStart_toStartOf="@+id/lblNombre"
        app:layout_constraintTop_toBottomOf="@+id/lblNombre" />

    <TextView
        android:id="@+id/lblApellido"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="44dp"
        android:layout_marginTop="40dp"
        android:text="Apellido:"
        app:layout_constraintStart_toStartOf="@+id/lblTitulo2"
        app:layout_constraintTop_toBottomOf="@+id/lblTitulo2" />

    <TextView
        android:id="@+id/lblMostrarApellido"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="19dp"
        android:text="----"
        app:layout_constraintStart_toStartOf="@+id/lblApellido"
        app:layout_constraintTop_toBottomOf="@+id/lblApellido" />

    <TextView
        android:id="@+id/lblRun"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="18dp"
        android:text="RUN:"
        app:layout_constraintBottom_toTopOf="@+id/lblMostrarRun"
        app:layout_constraintEnd_toEndOf="@+id/lblMostrarRun"
        app:layout_constraintStart_toEndOf="@+id/lblMostrarRun" />

    <TextView
        android:id="@+id/lblMostrarRun"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="55dp"
        android:text="----"
        app:layout_constraintBottom_toTopOf="@+id/lblEdad"
        app:layout_constraintEnd_toEndOf="@+id/lblMostrarNombre"
        app:layout_constraintStart_toStartOf="@+id/lblMostrarNombre" />

    <TextView
        android:id="@+id/lblEdad"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="321dp"
        android:text="Edad:"
        app:layout_constraintEnd_toEndOf="@+id/lblMostrarEdad"
        app:layout_constraintStart_toEndOf="@+id/lblMostrarEdad"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/lblMostrarEdad"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="44dp"
        android:layout_marginTop="16dp"
        android:text="----"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/lblEdad" />

    <TextView
        android:id="@+id/lblSexo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="70dp"
        android:text="Sexo:"
        app:layout_constraintStart_toStartOf="@+id/lblMostrarApellido"
        app:layout_constraintTop_toBottomOf="@+id/lblMostrarApellido" />

    <TextView
        android:id="@+id/lblMostrarSexo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:layout_marginTop="18dp"
        android:text="----"
        app:layout_constraintStart_toStartOf="@+id/lblSexo"
        app:layout_constraintTop_toBottomOf="@+id/lblSexo" />

    <TextView
        android:id="@+id/lblGeneroJuego"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="48dp"
        android:layout_marginEnd="18dp"
        android:text="GÃ©nero de Juego Favorito:"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/lblMostrarSexo" />

    <TextView
        android:id="@+id/lblMostrarGeneroJuego"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="23dp"
        android:text="----"
        app:layout_constraintStart_toStartOf="@+id/lblGeneroJuego"
        app:layout_constraintTop_toBottomOf="@+id/lblGeneroJuego" />

    <Button
        android:id="@+id/btnBotonAnterior"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="88dp"
        android:text="Anterior"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/btnBotonSiguiente"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="11dp"
        android:text="Siguiente"
        app:layout_constraintBottom_toBottomOf="@+id/btnBotonAnterior"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/btnBotonAnterior" />

    <Button
        android:id="@+id/btnVolverMain2"
        android:layout_width="0dp"
        android:layout_height="37dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="35dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="16dp"
        android:text="Volver"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnBotonSiguiente" />

    <TextView
        android:id="@+id/lblActual"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="124dp"
        android:layout_marginBottom="125dp"
        android:text="Actual"
        app:layout_constraintBottom_toBottomOf="@+id/btnBotonAnterior"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/lblMostrarGeneroJuego" />

    <TextView
        android:id="@+id/lblPrimerRegistro"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="13dp"
        android:layout_marginTop="10dp"
        android:text="0"
        app:layout_constraintStart_toEndOf="@+id/btnBotonAnterior"
        app:layout_constraintTop_toTopOf="@+id/btnBotonAnterior" />

    <TextView
        android:id="@+id/lblRegistroActual"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0"
        app:layout_constraintBaseline_toBaselineOf="@+id/lblUltimoRegistro"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/lblUltimoRegistro"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="18dp"
        android:text="0"
        app:layout_constraintEnd_toStartOf="@+id/btnBotonSiguiente"
        app:layout_constraintTop_toTopOf="@+id/btnBotonSiguiente" />
</androidx.constraintlayout.widget.ConstraintLayout>